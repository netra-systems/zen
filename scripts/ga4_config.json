  ],
  "audiences": [
    {
      "display_name": "Active Free Users",
      "description": "Free tier users active in last 7 days",
      "filter_clauses": [
        {
          "simple_filter": {
            "filter_expression": {
              "and_group": {
                "expressions": [
                  {
                    "dimension_filter": {
                      "dimension_name": "customUser:user_tier",
                      "string_filter": {
                        "match_type": "EXACT",
                        "value": "free"
                      }
                    }
                  }
                ]
              }
            }
          }
        }
      ],
      "membership_duration_days": 30
    },
    {
      "display_name": "Trial Ready to Convert",
      "description": "Trial users with high engagement",
      "filter_clauses": [
        {
          "simple_filter": {
            "filter_expression": {
              "and_group": {
                "expressions": [
                  {
                    "dimension_filter": {
                      "dimension_name": "customUser:user_tier",
                      "string_filter": {
                        "match_type": "EXACT",
                        "value": "free"
                      }
                    }
                  },
                  {
                    "metric_filter": {
                      "metric_name": "eventCount",
                      "numeric_filter": {
                        "operation": "GREATER_THAN",
                        "value": {
                          "int64_value": 10
                        }
                      }
                    }
                  }
                ]
              }
            }
          }
        }
      ],
      "membership_duration_days": 30
    },
    {
      "display_name": "Paid Users",
      "description": "All paying customers",
      "filter_clauses": [
        {
          "simple_filter": {
            "filter_expression": {
              "or_group": {
                "expressions": [
                  {
                    "dimension_filter": {
                      "dimension_name": "customUser:user_tier",
                      "string_filter": {
                        "match_type": "IN_LIST",
                        "value": "early,mid,enterprise"
                      }
                    }
                  }
                ]
              }
            }
          }
        }
      ],
      "membership_duration_days": 540
    },
    {
      "display_name": "High Value Prospects",
      "description": "Users showing enterprise behaviors",
      "filter_clauses": [
        {
          "simple_filter": {
            "filter_expression": {
              "and_group": {
                "expressions": [
                  {
                    "metric_filter": {
                      "metric_name": "customEvent:agent_activation_count",
                      "numeric_filter": {
                        "operation": "GREATER_THAN_OR_EQUAL",
                        "value": {
                          "int64_value": 5
                        }
                      }
                    }
                  },
                  {
                    "metric_filter": {
                      "metric_name": "customEvent:session_duration_seconds",
                      "numeric_filter": {
                        "operation": "GREATER_THAN_OR_EQUAL",
                        "value": {
                          "int64_value": 300
                        }
                      }
                    }
                  }
                ]
              }
            }
          }
        }
      ],
      "membership_duration_days": 60
    },
    {
      "display_name": "Churn Risk",
      "description": "Paid users with declining usage",
      "filter_clauses": [
        {
          "sequence_filter": {
            "sequence_steps": [
              {
                "constraint_filter": {
                  "dimension_filter": {
                    "dimension_name": "customUser:user_tier",
                    "string_filter": {
                      "match_type": "IN_LIST",
                      "value": "early,mid"
                    }
                  }
                }
              },
              {
                "constraint_filter": {
                  "metric_filter": {
                    "metric_name": "daysInactive",
                    "numeric_filter": {
                      "operation": "GREATER_THAN_OR_EQUAL",
                      "value": {
                        "int64_value": 14
                      }
                    }
                  }
                }
              }
            ]
          }
        }
      ],
      "membership_duration_days": 30
    },
    {
      "display_name": "Power Users",
      "description": "Highly engaged users",
      "filter_clauses": [
        {
          "simple_filter": {
            "filter_expression": {
              "and_group": {
                "expressions": [
                  {
                    "metric_filter": {
                      "metric_name": "customEvent:message_count",
                      "numeric_filter": {
                        "operation": "GREATER_THAN_OR_EQUAL",
                        "value": {
                          "int64_value": 50
                        }
                      }
                    }
                  },
                  {
                    "metric_filter": {
                      "metric_name": "customEvent:agent_activation_count",
                      "numeric_filter": {
                        "operation": "GREATER_THAN_OR_EQUAL",
                        "value": {
                          "int64_value": 20
                        }
                      }
                    }
                  }
                ]
              }
            }
          }
        }
      ],
      "membership_duration_days": 90
    }
  ],
