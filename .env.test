# Test Environment Configuration
# Optimized for fast E2E testing and agent startup

# Auth Service Configuration
AUTH_FAST_TEST_MODE=true
AUTH_SERVICE_ENABLED=true
AUTH_SERVICE_URL=http://127.0.0.1:8081
AUTH_CACHE_TTL_SECONDS=60

# Environment Settings
ENVIRONMENT=test
SQL_ECHO=false

# JWT Configuration (test only)
JWT_SECRET=test-secret-key-for-testing-only-do-not-use-in-production
JWT_ALGORITHM=HS256
JWT_ACCESS_EXPIRY_MINUTES=15
JWT_REFRESH_EXPIRY_DAYS=1

# Database URLs (use in-memory for speed)
AUTH_DATABASE_URL=sqlite+aiosqlite:///:memory:
DATABASE_URL=postgresql+asyncpg://postgres:DTprdt5KoQXlEG4Gh9lF@localhost:5433/netra_test

# Logging Configuration
LOG_LEVEL=WARNING
DISABLE_AUTH_LOGS=true

# Performance Optimizations
DISABLE_STARTUP_CHECKS=true
SKIP_MIGRATIONS=true
FAST_STARTUP_MODE=true

# Test Service Configuration
SERVICE_ID=backend-test
SERVICE_SECRET=test-service-secret

# CORS Configuration
CORS_ORIGINS=*

# Disable non-essential services for testing
CLICKHOUSE_MODE=disabled
REDIS_ENABLED=false
MONITORING_ENABLED=false