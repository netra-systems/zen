# Development Environment OAuth Configuration
ENVIRONMENT=development

# OAuth Client for Development
GOOGLE_OAUTH_CLIENT_ID_DEV="oauth-dev-client-id"
GOOGLE_OAUTH_CLIENT_SECRET_DEV="oauth-dev-client-secret"

# Development Features
ALLOW_DEV_LOGIN=true
ALLOW_MOCK_AUTH=true

# Frontend URLs
FRONTEND_URL="http://localhost:3000"
API_URL="http://localhost:8000"

# Database (local development)
# DATABASE_URL=postgresql+asyncpg://postgres:123@localhost:5432/netra
DATABASE_URL="postgresql://user@localhost/netra"
# REDIS_URL="redis://localhost:6379/0" # placeholder
# CLICKHOUSE_URL="clickhouse://localhost:9000/default" # placeholder

# Security (development only)
SECRET_KEY="dev-secret-key-change-in-production"
JWT_SECRET_KEY="dev-jwt-secret-change-in-production"

# Development Mode Flags
DEBUG=true
RELOAD=true

# DEV_MODE_DISABLE_REDIS=true
# DEV_MODE_DISABLE_CLICKHOUSE=true
# DEV_MODE_DISABLE_LLM=true