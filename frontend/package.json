{
  "name": "netra-frontend-apex-v1",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "dev:webpack": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "node run-test.js --config jest.config.unified.cjs",
    "test:unit": "node run-test.js TEST_SUITE=unit --config jest.config.unified.cjs",
    "test:integration": "node run-test.js TEST_SUITE=integration JEST_TIMEOUT=60000 --config jest.config.unified.cjs",
    "test:critical": "node run-test.js TEST_SUITE=critical --config jest.config.unified.cjs",
    "test:visual": "node run-jest.js --config jest.config.visual.cjs",
    "test:performance": "node run-jest.js --config jest.config.performance.cjs --testNamePattern=\"performance\"",
    "test:parallel": "node run-test.js MAX_WORKERS=auto --config jest.config.unified.cjs",
    "test:sequential": "node run-test.js MAX_WORKERS=1 --config jest.config.unified.cjs",
    "test:fast": "node run-test.js TEST_SUITE=unit MAX_WORKERS=auto --config jest.config.unified.cjs --silent",
    "test:watch": "node run-test.js --config jest.config.unified.cjs --watch",
    "test:ci": "node run-test.js MAX_WORKERS=4 --config jest.config.unified.cjs --ci --silent --bail",
    "test:update-snapshots": "node run-test.js UPDATE_SNAPSHOTS=true TEST_SUITE=visual --config jest.config.unified.cjs -u",
    "test:clear-cache": "node node_modules/jest/bin/jest.js --clearCache",
    "test:suites": "node test-suite-runner.js --all",
    "test:suites:components": "node run-jest.js --config jest.config.suites.cjs --selectProjects=components",
    "test:suites:chat": "node run-jest.js --config jest.config.suites.cjs --selectProjects=chat",
    "test:suites:hooks": "node run-jest.js --config jest.config.suites.cjs --selectProjects=hooks",
    "test:suites:auth": "node run-jest.js --config jest.config.suites.cjs --selectProjects=auth",
    "test:suites:integration": "node test-suite-runner.js integration-basic integration-advanced integration-comprehensive integration-critical",
    "test:suites:fast": "node test-suite-runner.js --priority 1",
    "test:suites:medium": "node test-suite-runner.js --priority 2",
    "test:suites:slow": "node test-suite-runner.js --priority 4",
    "test:suites:parallel": "node test-suite-runner.js --all --max-parallel 8",
    "test:suites:sequential": "node test-suite-runner.js --all --sequential",
    "test:suites:watch": "node run-jest.js --config jest.config.suites.cjs --watch",
    "test:suites:coverage": "node run-jest.js --config jest.config.suites.cjs --coverage",
    "test:ultra": "node run-jest.js --config jest.config.ultra.cjs --silent",
    "test:ultra:watch": "node run-jest.js --config jest.config.ultra.cjs --watch --changedSince=main",
    "test:ultra:coverage": "node run-jest.js --config jest.config.ultra.cjs --coverage",
    "test:optimize": "node test-optimizer.js",
    "test:optimize:bail": "node test-optimizer.js --bail",
    "test:optimize:coverage": "node test-optimizer.js --coverage",
    "test:shard": "node test-shard-runner.js",
    "test:shard:coverage": "node test-shard-runner.js --coverage",
    "test:shard:aggregate": "node test-shard-runner.js --aggregate",
    "test:validate-ultra": "node validate-ultra-config.js",
    "test:smoke": "node run-jest.js --config jest.config.smoke.cjs --silent",
    "test:integration:auth-flow": "node run-jest.js --config jest.config.integration.cjs --testNamePattern=\"auth.*flow\"",
    "test:integration:websocket": "node run-jest.js --config jest.config.integration.cjs --testNamePattern=\"websocket\"",
    "test:integration:store-sync": "node run-jest.js --config jest.config.integration.cjs --testNamePattern=\"store.*sync\"",
    "test:integration:api-calls": "node run-jest.js --config jest.config.integration.cjs --testNamePattern=\"api.*call\"",
    "test:visual:update": "node run-jest.js --config jest.config.visual.cjs --updateSnapshot",
    "test:a11y": "node run-jest.js --config jest.config.cjs --selectProjects=a11y",
    "test:comprehensive": "node run-jest.js --config jest.config.extended.cjs --coverage",
    "test:ci:smoke": "npm run test:smoke -- --ci --bail --silent",
    "test:ci:unit": "npm run test:unit -- --ci --coverage --silent",
    "test:ci:integration": "npm run test:integration -- --ci --coverage",
    "test:ci:comprehensive": "npm run test:comprehensive -- --ci --maxWorkers=2",
    "coverage:merge": "nyc merge coverage coverage/merged-coverage.json",
    "coverage:report": "nyc report --reporter=html --reporter=text --temp-dir=coverage",
    "cypress:open": "cypress open",
    "cy:run": "cypress run",
    "cy:test": "start-server-and-test dev http://localhost:3000 cypress:run",
    "cypress:run": "cypress run",
    "e2e:test": "playwright test",
    "e2e:test:ui": "playwright test --ui",
    "e2e:test:headed": "playwright test --headed",
    "e2e:test:debug": "playwright test --debug",
    "e2e:test:chrome": "playwright test --project=chromium",
    "e2e:test:firefox": "playwright test --project=firefox",
    "e2e:test:safari": "playwright test --project=webkit",
    "e2e:test:mobile": "playwright test --project=mobile-chrome",
    "e2e:ci": "playwright test --reporter=dot"
  },
