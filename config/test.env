# Test Environment Configuration
# This file defines environment variables for test environment
# Uses explicit test configurations - NO fallbacks to development

# Environment Setting
ENVIRONMENT=test

# Database Configuration
POSTGRES_USER=netra_test
POSTGRES_PASSWORD=netra_test_password
POSTGRES_DB=netra_test
POSTGRES_HOST=localhost
POSTGRES_PORT=5434
# #removed-legacyis now constructed from individual components per database_connectivity_architecture.xml
# DATABASE_URL=postgresql://netra_test:netra_test_password@localhost:5434/netra_test

# Redis Configuration  
REDIS_HOST=localhost
REDIS_PORT=6381
REDIS_URL=redis://localhost:6381/0

# ClickHouse Configuration (optional for test)
CLICKHOUSE_HOST=localhost
CLICKHOUSE_PORT=8123
CLICKHOUSE_USER=netra_test
CLICKHOUSE_PASSWORD=netra_test_password
CLICKHOUSE_DB=netra_test_analytics
CLICKHOUSE_SECURE=false

# Service Configuration
SERVER_PORT=8000
AUTH_PORT=8081
FRONTEND_PORT=3000
LOG_LEVEL=DEBUG

# Authentication Configuration (test defaults)
SECRET_KEY=test-secret-key-for-test-environment-only-32-chars-min
JWT_SECRET_KEY=test-jwt-secret-key-for-test-environment-only-32-chars-min
FERNET_KEY=test-fernet-key-for-test-environment-only-32-chars
SERVICE_SECRET=test-service-secret-for-test-environment
SERVICE_ID=netra-test

# JWT Configuration
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7

# Feature Flags for Test
ALLOW_DEV_OAUTH_SIMULATION=true
WEBSOCKET_AUTH_BYPASS=true
SKIP_MIGRATION_ON_STARTUP=false
ENABLE_STARTUP_CHECKS=true
TEST_MODE=true
TESTING=1

# API Configuration for Test
API_BASE_URL=http://localhost:8000
AUTH_SERVICE_URL=http://localhost:8081
WS_BASE_URL=ws://localhost:8000/ws
FRONTEND_URL=http://localhost:3000

# CORS Configuration
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# OAuth Configuration (test environment - explicit named configs)
# These are test-specific OAuth configurations
GOOGLE_OAUTH_CLIENT_ID_TEST=test-oauth-client-id-for-automated-testing
GOOGLE_OAUTH_CLIENT_SECRET_TEST=test-oauth-client-secret-for-automated-testing
OAUTH_REDIRECT_URI=http://localhost:8081/oauth/callback
OAUTH_CALLBACK_URL=http://localhost:8081/oauth/callback

# Health Check Configuration
HEALTH_CHECK_GRACE_PERIOD=30
HEALTH_CHECK_MAX_RETRIES=3

# E2E Test Configuration
# Used for E2E tests to bypass OAuth authentication
E2E_OAUTH_SIMULATION_KEY=test-e2e-oauth-bypass-key-for-testing-only

# LLM API Keys (test placeholders)
ANTHROPIC_API_KEY=test-anthropic-api-key
OPENAI_API_KEY=test-openai-api-key
GEMINI_API_KEY=test-gemini-api-key