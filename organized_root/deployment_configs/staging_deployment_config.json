{
  "deployment": {
    "project_id": "netra-staging",
    "region": "us-central1",
    "services": {
      "backend": "netra-backend-staging",
      "frontend": "netra-frontend-staging",
      "auth": "netra-auth-service"
    },
    "urls": {
      "api": "https://api.staging.netrasystems.ai",
      "frontend": "https://app.staging.netrasystems.ai",
      "auth": "https://auth.staging.netrasystems.ai",
      "websocket": "wss://api.staging.netrasystems.ai/ws"
    }
  },
  "environment_variables": {
    "backend": {
      "ENVIRONMENT": "staging",
      "SERVICE_NAME": "backend",
      "LOG_LEVEL": "INFO",
      "PYTHONUNBUFFERED": "1",
      "API_BASE_URL": "https://api.staging.netrasystems.ai",
      "FRONTEND_URL": "https://app.staging.netrasystems.ai",
      "AUTH_SERVICE_URL": "https://auth.staging.netrasystems.ai",
      "AUTH_SERVICE_ENABLED": "true",
      "K_SERVICE": "netra-backend-staging",
      "CORS_ORIGINS": "https://app.staging.netrasystems.ai,https://api.staging.netrasystems.ai",
      "REDIS_MODE": "shared",
      "CLICKHOUSE_MODE": "shared",
      "LLM_MODE": "shared"
    },
    "frontend": {
      "NODE_ENV": "production",
      "NEXT_PUBLIC_API_URL": "https://api.staging.netrasystems.ai",
      "NEXT_PUBLIC_WS_URL": "wss://api.staging.netrasystems.ai/ws",
      "NEXT_PUBLIC_AUTH_URL": "https://auth.staging.netrasystems.ai",
      "NEXT_PUBLIC_ENVIRONMENT": "staging",
      "ENVIRONMENT": "staging",
      "SERVICE_NAME": "frontend",
      "LOG_LEVEL": "INFO"
    },
    "auth": {
      "ENVIRONMENT": "staging",
      "SERVICE_NAME": "auth",
      "LOG_LEVEL": "INFO",
      "PYTHONUNBUFFERED": "1",
      "PORT": "8001",
      "FRONTEND_URL": "https://app.staging.netrasystems.ai",
      "API_URL": "https://api.staging.netrasystems.ai",
      "K_SERVICE": "netra-auth-service",
      "CORS_ORIGINS": "https://app.staging.netrasystems.ai,https://api.staging.netrasystems.ai"
    }
  },
  "secrets": {
    "backend": [
      "DATABASE_URL=database-url-staging:latest",
      "REDIS_URL=redis-url-staging:latest",
      "CLICKHOUSE_URL=clickhouse-url-staging:latest",
      "GEMINI_API_KEY=gemini-api-key-staging:latest",
      "JWT_SECRET_KEY=jwt-secret-key-staging:latest",
      "FERNET_KEY=fernet-key-staging:latest",
      "LANGFUSE_SECRET_KEY=langfuse-secret-key-staging:latest",
      "LANGFUSE_PUBLIC_KEY=langfuse-public-key-staging:latest",
      "GITHUB_TOKEN=github-token-staging:latest"
    ],
    "auth": [
      "DATABASE_URL=database-url-staging:latest",
      "JWT_SECRET_KEY=jwt-secret-key-staging:latest",
      "FERNET_KEY=fernet-key-staging:latest",
      "GOOGLE_CLIENT_ID=google-client-id-staging:latest",
      "GOOGLE_CLIENT_SECRET=google-client-secret-staging:latest"
    ]
  },
  "resources": {
    "backend": {
      "memory": "1Gi",
      "cpu": "1",
      "min_instances": 0,
      "max_instances": 10
    },
    "frontend": {
      "memory": "512Mi",
      "cpu": "1",
      "min_instances": 0,
      "max_instances": 10
    },
    "auth": {
      "memory": "1Gi",
      "cpu": "1",
      "min_instances": 1,
      "max_instances": 2
    }
  }
}