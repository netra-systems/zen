{
  "terminals": [
    {
      "name": "Main Development",
      "args": [
        "--project",
        "netra-apex"
      ],
      "working_dir": null
    },
    {
      "name": "Testing Terminal",
      "args": [
        "--project",
        "netra-apex",
        "--context",
        "testing"
      ],
      "working_dir": null
    },
    {
      "name": "Debugging Session",
      "args": [
        "--project",
        "netra-apex",
        "--context",
        "debug"
      ],
      "working_dir": null
    }
  ],
  "terminal_type": "auto",
