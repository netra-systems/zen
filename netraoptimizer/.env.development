# NetraOptimizer Development Environment Configuration
# For local development with CloudSQL via proxy

# Environment
ENVIRONMENT=development

# CloudSQL via Cloud SQL Proxy
USE_CLOUD_SQL=true
POSTGRES_HOST=localhost
POSTGRES_PORT=5434
POSTGRES_USER=postgres
POSTGRES_PASSWORD=DTprdt5KoQXlEG4Gh9lF  # Replace with your password

# Claude configuration
CLAUDE_EXECUTABLE=claude
CLAUDE_TIMEOUT=300  # Shorter timeout for development

# Analytics
NETRA_ENABLE_ANALYTICS=true
NETRA_BATCH_PREDICTION=false

# Development settings
NETRA_MAX_CONCURRENT=5
NETRA_STARTUP_DELAY=0

# Debug logging
NETRA_LOG_LEVEL=DEBUG

# Optional Redis for caching
# NETRA_REDIS_HOST=localhost
# NETRA_REDIS_PORT=6379