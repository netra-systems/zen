# Development Environment OAuth Configuration
ENVIRONMENT=development

# OAuth Client for Development
GOOGLE_OAUTH_CLIENT_ID_DEV=your-dev-client-id-here
GOOGLE_OAUTH_CLIENT_SECRET_DEV=your-dev-client-secret-here

# Development Features
ALLOW_DEV_LOGIN=true
ALLOW_MOCK_AUTH=true

# Frontend URLs
FRONTEND_URL=http://localhost:3000
API_URL=http://localhost:8000

# Database (local development)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/netra_dev
REDIS_URL=redis://localhost:6379/0
CLICKHOUSE_URL=clickhouse://localhost:9000/default

# Security (development only)
SECRET_KEY=dev-secret-key-change-in-production
JWT_SECRET_KEY=dev-jwt-secret-change-in-production

# Development Mode Flags
DEBUG=true
RELOAD=true