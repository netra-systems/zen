[run]
source = netra_backend/app
omit = 
    */tests/*
    */test_*.py
    */__pycache__/*
    */venv/*
    */migrations/*
    */alembic/*
    netra_backend/app/config.py
    netra_backend/app/main.py
    dev_launcher/*
    test_framework/*
    scripts/*
    organized_root/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod
    except ImportError:
    pass

ignore_errors = True
precision = 2
show_missing = True
skip_covered = False

[html]
directory = reports/coverage/html

[json]
output = reports/coverage/coverage.json

[xml]
output = reports/coverage/coverage.xml