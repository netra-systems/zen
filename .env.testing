# Testing Environment OAuth Configuration
ENVIRONMENT=testing
TESTING=true

# OAuth Client for Testing
GOOGLE_OAUTH_CLIENT_ID_TEST=test-client-id-here
GOOGLE_OAUTH_CLIENT_SECRET_TEST=test-client-secret-here

# Testing Features
ALLOW_DEV_LOGIN=false
ALLOW_MOCK_AUTH=true

# Frontend URLs
FRONTEND_URL=http://test.local
API_URL=http://api.test.local

# Database (test isolation)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/netra_test
REDIS_URL=redis://localhost:6379/1
CLICKHOUSE_URL=clickhouse://localhost:9000/test

# Security (testing)
SECRET_KEY=test-secret-key
JWT_SECRET_KEY=test-jwt-secret

# Testing Mode Flags
DEBUG=false
RELOAD=false
TEST_MODE=true